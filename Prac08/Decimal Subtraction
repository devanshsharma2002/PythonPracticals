from decimal import Decimal

def subtract_decimal(a, b):
    # Convert inputs to Decimal type for precise arithmetic
    num1 = Decimal(a)
    num2 = Decimal(b)
    
    # Perform subtraction
    result = num1 - num2
    
    # Print result, retaining the decimal form
    if result < 0:
        print(f"Result: -{abs(result)}")
    else:
        print(f"Result: {result}")

